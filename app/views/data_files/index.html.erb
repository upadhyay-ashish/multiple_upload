<h1>Listing Hashtags with Banners</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>

<% @data_files.each do |data_file| %>
  <tr>
    <td><%=h data_file.title %></td>
    <td><%=h data_file.description %></td>
    <td><%= link_to 'Show', data_file %></td>
    <td><%= link_to 'Edit', edit_data_file_path(data_file) %></td>
    <td><%= link_to 'Destroy', data_file, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'Upload Banner for a Hashtag', new_data_file_path %>